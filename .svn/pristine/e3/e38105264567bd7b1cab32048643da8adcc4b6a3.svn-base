package br.gov.ma.detran.examespraticosmobile.holder;

import android.view.View;
import android.widget.ImageButton;
import android.widget.TextView;

import br.gov.ma.detran.examespraticosmobile.R;

public class ListViewFecharHolder {

    public TextView mNome;
    public TextView mResultado;
    public ImageButton mBotao;

    public ListViewFecharHolder(View v) {
        this.mNome = v.findViewById(R.id.txtNomeCandidato);
        this.mResultado = v.findViewById(R.id.txtResultado);
        this.mBotao = v.findViewById(R.id.btnFecharProva);
    }
}
