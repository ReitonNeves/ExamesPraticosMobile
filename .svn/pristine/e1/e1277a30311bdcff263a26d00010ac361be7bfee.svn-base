package br.gov.ma.detran.examespraticosmobile.holder;

import android.view.View;
import android.widget.ImageButton;
import android.widget.TextView;

import br.gov.ma.detran.examespraticosmobile.R;

public class ListViewFaltasHolder {
    public TextView mDescricao;
    public TextView mQuantidade;
    public ImageButton mBotaoAdd;
    public ImageButton mBotaoDelete;

    public ListViewFaltasHolder(View v){
        mDescricao = v.findViewById(R.id.txtDescricaoFalta);
        mQuantidade = v.findViewById(R.id.txtQuantidadeAtual);
        mBotaoAdd = v.findViewById(R.id.btnAddFalta);
        mBotaoDelete = v.findViewById(R.id.btnRemoveFalta);
    }
}
